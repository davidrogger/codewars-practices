{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./practices",
    "outDir": "./build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "practices"
  ],
  "exclude": [
    "node_modules"
  ]
}